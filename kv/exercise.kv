#:kivy 2.1.0
<Exercise>:
    name:"exercise"

    Image:
        source:"images/testi3.png"
        allow_stretch:True
        keep_ratio:False
    MDIconButton:
        icon:"play"
        icon_size:"50dp"
        pos_hint:{"x":0.6,"top":1}
        on_release:
            root.play()

    MDIconButton:
        icon:"pause"
        icon_size:"50dp"
        size_hint:0.5, 0.1
        pos_hint:{"x":0.7,"top":1}
        on_release:
            root.stop()
        
