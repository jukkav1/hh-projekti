#:kivy 2.1.0
<Exercise>:
    name:"exercise"

    Image:
        source:"images/tausta-lehdet.png"
        allow_stretch:True
        keep_ratio:False

    MDLabel: # ensimmäinen raita
        text:"Metsä"
        theme_text_color: "Custom"
        text_color:0,0,0,1
        font_style:"Subtitle2"
        font_size:"20sp"
        pos_hint:{"x":0.40,"top":1.45}
    
    MDIconButton:
        id:play_one
        name:"play one"
        icon:"play-circle-outline"
        icon_size:"48dp"
        pos_hint:{"center_x":0.08,"top":0.965}
        on_release:root.Button1("sounds/ambient_forest_new.mp3") #kutsuu def Button1 exercice.py:stä

    MDIconButton:
        id:stop_one
        name:"stop one"
        icon:"stop-circle-outline"
        icon_size:"48dp"
        pos_hint:{"center_x":0.20,"top":0.965}
        on_release:root.stop_audio() #kutsuu def play_stop_audiota exercice.py:stä
    
    MDProgressBar:
        pos_hint:{"center_x":0.62,"center_y":0.90}
        size_hint_x: 0.43
        color: 163/255, 102/255, 28/255, 1
        max:494
        value:root.progress1

    MDLabel:
        text:root.timer1 # hakee tiedot exercise.py:n timer kohdasta, vaatii string, muodon
        pos_hint:{"x":0.26,"top":1.40}

    MDLabel:
        text:"08:12" # tähän näkyviiin audion pituus
        pos_hint:{"x":0.86,"top":1.40}
    
    MDLabel: # ensimmäinen raita
        text:"Lehdet"
        theme_text_color: "Custom"
        text_color:0,0,0,1
        font_style:"Subtitle2"
        font_size:"20sp"
        pos_hint:{"x":0.40,"top":1.30}
    
    MDIconButton:
        id:play_two
        name:"play two"
        icon:"play-circle-outline"
        icon_size:"48dp"
        pos_hint:{"center_x":0.08,"top":0.815}
        on_release:root.Button2("sounds/lehdet.mp3") #kutsuu def Button2 exercice.py:stä

    MDIconButton:
        id:stop_one
        name:"stop one"
        icon:"stop-circle-outline"
        icon_size:"48dp"
        pos_hint:{"center_x":0.20,"top":0.815}
        on_release:root.stop_audio() #kutsuu def stop_audiota exercice.py:stä
    
    MDProgressBar:
        pos_hint:{"center_x":0.62,"center_y":0.75}
        size_hint_x: 0.43
        color: 163/255, 102/255, 28/255, 1
        max:494
        value:root.progress2

    MDLabel:
        text:root.timer2 # hakee tiedot exercise.py:n timer kohdasta, vaatii string, muodon
        pos_hint:{"x":0.26,"top":1.25}

    MDLabel:
        text:"07:52" # tähän näkyviiin audion pituus
        pos_hint:{"x":0.86,"top":1.25}























