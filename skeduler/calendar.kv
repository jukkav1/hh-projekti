#:kivy 2.1.0
<Button>:
	background_color: 0.1,.5,.5,1
	
<Calendar>:
	orientation:'vertical'
	canvas.before:
		Color:
			rgba: .5,.6,.7,1
		Rectangle:
			pos: self.pos
			size: self.size

	BoxLayout:
		size_hint:1,.1
		Select:
		
	BoxLayout:
		Months:
		BoxLayout:
			orientation: 'vertical'
			Days:
			Dates:

	BoxLayout:
		size_hint:1,.1
		Status: