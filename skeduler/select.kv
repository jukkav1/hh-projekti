#:kivy 2.1.0
<Select>:
	lbl: lbl
	btn: btn
	year: year
	
	Label:
		id: lbl
		text:'Valitse vuosi:'
		
	Spinner:
		id: year
		size_hint: .1,1
		halign: 'center'
		valign: 'middle'
		text_size: self.size
		text: '2023'
		on_press: root.get_years()
		
	Button:
		id: btn
		text:'Aseta'
		on_release: lbl.text = f'Valittu vuosi: {year.text}'
